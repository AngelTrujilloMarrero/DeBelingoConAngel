{
  "version": 2,
  "framework": null,
  "buildCommand": "mkdir -p empty_dist && echo 'API Backend Only' > empty_dist/index.txt",
  "outputDirectory": "empty_dist",
  "functions": {
    "api/**/*.js": {
      "memory": 1024,
      "maxDuration": 10
    }
  },
  "redirects": [
    {
      "source": "/",
      "destination": "https://debelingoconangel.web.app",
      "permanent": true
    },
    {
      "source": "/((?!api/).*)",
      "destination": "https://debelingoconangel.web.app/$1",
      "permanent": true
    }
  ]
}